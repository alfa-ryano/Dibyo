<?xml version="1.0" encoding="UTF-8"?>
<richtext version="1.0.0.0" xmlns="http://www.wxwidgets.org">
  <paragraphlayout textcolor="#000000" fontpointsize="9" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Segoe UI" alignment="1" parspacingafter="10" parspacingbefore="0" linespacing="10" margin-left="5,4098" margin-right="5,4098" margin-top="5,4098" margin-bottom="5,4098">
    <paragraph fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">
      <text fontpointsize="16" fontfamily="74" fontstyle="90" fontweight="92" fontunderlined="0" fontface="Arial">Preference Sheet [pn].</text>
    </paragraph>
    <paragraph fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">
      <text fontweight="92"></text>
    </paragraph>
    <paragraph fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">
      <text>"You are asked to state your preference over certain money proposed in the first column "</text>
      <text fontweight="92">"(Option A) "</text>
      <text fontweight="90">"or a lottery "</text>
      <text fontweight="92">(Option B)</text>
      <text fontweight="90">". There are 3 (three) answer options to represent your preference: i) "</text>
      <text fontweight="92">"I choose Option A; "</text>
      <text fontweight="90">"ii) "</text>
      <text fontweight="92">I am not sure what to choose;</text>
      <text fontweight="90">" iii) "</text>
      <text fontweight="92">"I choose Option B. "</text>
      <text fontweight="90">"Should you click "</text>
      <text fontweight="92">CONFIRM</text>
      <text fontweight="90">" once you have finished completing this Preference Sheet, otherwise click "</text>
      <text fontweight="92">"CLEAR "</text>
      <text fontweight="90">"to modify your answer. The "</text>
      <text fontweight="92">"CONFIRM "</text>
      <text fontweight="90">button will appear after 10 seconds. Please notice that you cannot go back to the previous Preference Sheet.</text>
    </paragraph>
    <paragraph fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">
      <text></text>
    </paragraph>
    <paragraph fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">
      <text fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="92" fontunderlined="0" fontface="Arial">Option A:</text>
      <text fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">" You will receive a "</text>
      <text textcolor="#FF0000" fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="92" fontunderlined="0" fontface="Arial">proposed amount of money</text>
      <text fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">" for sure."</text>
    </paragraph>
    <paragraph fontpointsize="13" fontfamily="70" fontstyle="90" fontweight="90" fontunderlined="0" fontface="Arial">
      <text fontpointsize="13" fontweight="92" fontface="Arial">Option B:</text>
      <text fontpointsize="13" fontface="Arial">" No option given"</text>
    </paragraph>
